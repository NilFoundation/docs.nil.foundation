{
  "rewrites": [
        { "source": "/(.*)", "destination": "src/app.js" }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/vercel_app_domain_name/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ],
  "redirects": [
    {
      "source": "/crypto3",
      "destination": "/crypto3/intro",
      "permanent": true
    },
    {
      "source": "/crypto3/",
      "destination": "/crypto3/intro",
      "permanent": true
    },
    {
      "source": "/zkllvm",
      "destination": "/zkllvm/overview/what-is-zkllvm",
      "permanent": true
    },
    {
      "source": "/zkllvm/",
      "destination": "/zkllvm/overview/what-is-zkllvm",
      "permanent": true
    },
    {
      "source": "/proof-market",
      "destination": "/proof-market/intro",
      "permanent": true
    },
    {
      "source": "/proof-market/",
      "destination": "/proof-market/intro",
      "permanent": true
    }
  ]
}
