{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "redirects": [
    {
      "source": "/crypto3",
      "destination": "/crypto3/intro",
      "permanent": true
    },
    {
      "source": "/crypto3/",
      "destination": "/crypto3/intro",
      "permanent": true
    },
    {
      "source": "/zkllvm",
      "destination": "/zkllvm/overview/what-is-zkllvm",
      "permanent": true
    },
    {
      "source": "/zkllvm/",
      "destination": "/zkllvm/overview/what-is-zkllvm",
      "permanent": true
    },
    {
      "source": "/proof-market",
      "destination": "/proof-market/intro",
      "permanent": true
    },
    {
      "source": "/proof-market/",
      "destination": "/proof-market/intro",
      "permanent": true
    }
  ]
    }
  ]
  
}
